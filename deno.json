{
  "tasks": {
    "start": "deno run -A --unstable mod.js",
    "start-caddy": "caddy start --config src-caddy/Caddyfile",
    "start-native": "cargo tauri dev",
    "build-native": "cargo tauri build",
    "start-ui": "cd src-ui && deno task develop-client",
    "build-ui": "cd src-ui && deno task build-client",
    "start-tag": "cd src-tag && deno task start",
    "start-edge": "deno task build-ui && cd src-edge && deno task start",
    "start-gun": "cd src-gun && deno task start",
    "start-owncast": "cd src-owncast && deno task start",
    "start-statebus": "cd src-statebus && deno task start",
    "start-solid": "cd src-solid && deno task start",
    "package-tag": "cd src-tag && deno task package",
    "publish-tag": "cd src-tag && deno task publish"
  },
  "compilerOptions": {
  }
}
